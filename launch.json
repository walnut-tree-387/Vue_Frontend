{
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Vue.js: Chrome",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:8082",
            "webRoot": "${workspaceFolder}/src",
            "sourceMapPathOverrides": {
                "webpack:///src/*": "${webRoot}/*"
            }
        },
        {
            "name": "Vue.js: Attach",
            "type": "chrome",
            "request": "attach",
            "port": 9222,
            "webRoot": "${workspaceFolder}/src",
            "sourceMapPathOverrides": {
                "webpack:///src/*": "${webRoot}/*"
            }
        }
    ]
}
